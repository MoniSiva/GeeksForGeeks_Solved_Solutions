class GfG
{
    void printPat(int n) {
        
        int num;
        for(int i = n ;i > 0 ; i--){
            for(int j = n; j > 0 ; j--) {
                for(int k = 0; k < i ; k++ ){
                    System.out.print(j+" ");
                }
            }
            System.out.print("$");
        }
    }
}
