



/*Complete the Function below*/

class GfG

{

	public static int palinArray(int[] a, int n) {    

	    for(int i = 0 ; i < n ; i++){

	        int value = a[i];

	        int rev = 0;

	        while(value > 0){

	            int mod = value%10;

	            rev = (rev*10)+mod;

	            value = value/10;

	        }

	        if(a[i] != rev)

	            return 0;

	    }

        return 1;         

    }

}
